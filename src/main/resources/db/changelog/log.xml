<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <changeSet id="1" author="PedroCostaa">
        <createTable tableName="users">
            <column name="id" type="BIGINT" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="username" type="VARCHAR(255)">
                <constraints nullable="false" unique="true"/>
            </column>
            <column name="password" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="enabled" type="BOOLEAN">
                <constraints nullable="false"/>
            </column>
            <column name="first_access" type="BOOLEAN">
                <constraints nullable="false"/>
            </column>
        </createTable>

        <createTable tableName="authorities">
            <column name="id" type="BIGINT" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="authority" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="user_id" type="BIGINT">
                <constraints nullable="false"/>
            </column>
        </createTable>

        <createTable tableName="suppliers">
            <column name="id" type="BIGINT" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="name" type="VARCHAR(255)">
                <constraints nullable="false" unique="true"/>
            </column>
            <column name="status" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="orders" type="INT"/>
            <column name="orders_not_delivered" type="INT"/>
            <column name="orders_not_delivered_percentage" type="FLOAT"/>
            <column name="total_value" type="FLOAT"/>
            <column name="value_received" type="FLOAT"/>
            <column name="value_received_percentage" type="FLOAT"/>
            <column name="average_value_received" type="FLOAT"/>
            <column name="min_value_received" type="FLOAT"/>
        </createTable>

        <createTable tableName="products">
            <column name="id" type="BIGINT" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="code" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="name" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="supplier_id" type="BIGINT">
                <constraints nullable="false"/>
            </column>
            <column name="barcode" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="store" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="out_of_mix" type="BOOLEAN">
                <constraints nullable="false"/>
            </column>
            <column name="weight" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="sector" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="group_name" type="VARCHAR(255)"/>
            <column name="subgroup" type="VARCHAR(255)"/>
            <column name="package_quantity" type="INT"/>
            <column name="minimum_stock" type="INT">
                <constraints nullable="false"/>
            </column>
            <column name="sales_last30days" type="FLOAT">
                <constraints nullable="false"/>
            </column>
            <column name="sales_last12months" type="FLOAT">
                <constraints nullable="false"/>
            </column>
            <column name="sales_last7days" type="FLOAT">
                <constraints nullable="false"/>
            </column>
            <column name="daily_sales" type="FLOAT">
                <constraints nullable="false"/>
            </column>
            <column name="last_cost" type="FLOAT">
                <constraints nullable="false"/>
            </column>
            <column name="average_sales_last30days" type="FLOAT">
                <constraints nullable="false"/>
            </column>
            <column name="current_stock" type="FLOAT"/>
            <column name="open_order" type="FLOAT">
                <constraints nullable="false"/>
            </column>
            <column name="expiration_date" type="DATE"/>
            <column name="loss_quantity" type="FLOAT">
                <constraints nullable="false"/>
            </column>
            <column name="promotion_type" type="VARCHAR(255)"/>
            <column name="brand" type="VARCHAR(255)"/>
            <column name="exchange_quantity" type="FLOAT">
                <constraints nullable="false"/>
            </column>
            <column name="flag1" type="VARCHAR(255)"/>
            <column name="flag2" type="VARCHAR(255)"/>
            <column name="flag3" type="VARCHAR(255)"/>
            <column name="flag4" type="VARCHAR(255)"/>
            <column name="flag5" type="VARCHAR(255)"/>
            <column name="average_expiration" type="FLOAT">
                <constraints nullable="false"/>
            </column>
            <column name="network_stock" type="FLOAT">
                <constraints nullable="false"/>
            </column>
            <column name="transfer_package" type="INT">
                <constraints nullable="false"/>
            </column>
            <column name="promotion_quantity" type="FLOAT">
                <constraints nullable="false"/>
            </column>
            <column name="category" type="VARCHAR(255)"/>
            <column name="no_delivery_quantity" type="INT">
                <constraints nullable="false"/>
            </column>
            <column name="average_sales30d12m" type="FLOAT">
                <constraints nullable="false"/>
            </column>
            <column name="highest_sales" type="FLOAT"/>
            <column name="daily_sales_amount" type="FLOAT">
                <constraints nullable="false"/>
            </column>
            <column name="days_to_expire" type="FLOAT">
                <constraints nullable="false"/>
            </column>
            <column name="sales_projection" type="FLOAT">
                <constraints nullable="false"/>
            </column>
            <column name="in_projection" type="INT">
                <constraints nullable="false"/>
            </column>
            <column name="excess_stock" type="FLOAT">
                <constraints nullable="false"/>
            </column>
            <column name="total_cost" type="FLOAT"/>
            <column name="total_sales" type="FLOAT"/>
            <column name="term" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="current_stock_per_package" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="average_sales" type="FLOAT">
                <constraints nullable="false"/>
            </column>
            <column name="cost_p" type="FLOAT"/>
            <column name="sales_p" type="FLOAT"/>
            <column name="available_stock" type="FLOAT">
                <constraints nullable="false"/>
            </column>
            <column name="stock_turnover" type="FLOAT"/>
            <column name="net_cost" type="FLOAT"/>
            <column name="sales_price" type="FLOAT"/>
            <column name="sales_price2" type="FLOAT"/>
            <column name="promotion_price" type="FLOAT"/>
        </createTable>

        <createTable tableName="extra_orders">
            <column name="id" type="BIGINT" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="user_id" type="BIGINT">
                <constraints nullable="false"/>
            </column>
            <column name="supplier_id" type="BIGINT">
                <constraints nullable="false"/>
            </column>
            <column name="stores" type="BIGINT" defaultValue="0">
                <constraints nullable="false"/>
            </column>
            <column name="partial_complete" type="varchar(255)">
                <constraints nullable="false"/>
            </column>
            <column name="processed" type="BOOLEAN">
                <constraints nullable="false"/>
            </column>
            <column name="date_submitted" type="DATE">
                <constraints nullable="false"/>
            </column>
            <column name="products" type="BIGINT"/>
            <column name="origin" type="VARCHAR(255)"/>
        </createTable>

        <createTable tableName="token_blacklist">
            <column name="id" type="BIGINT" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="token" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="user_id" type="BIGINT">
                <constraints nullable="false"/>
            </column>
            <column name="created_at" type="DATE">
                <constraints nullable="false"/>
            </column>
        </createTable>

        <createTable tableName="extra_order_stores">
            <column name="id" type="BIGINT" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="store" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="extra_order_id" type="BIGINT">
                <constraints nullable="false"/>
            </column>
        </createTable>

        <createTable tableName="extra_order_products">

            <column name="id" type="BIGINT" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="product_id" type="BIGINT">
                <constraints nullable="false"/>
            </column>
            <column name="price" type="DOUBLE">
                <constraints nullable="false"/>
            </column>
            <column name="quantity" type="INT">
                <constraints nullable="false"/>
            </column>
            <column name="extra_order_id" type="BIGINT">
                <constraints nullable="false"/>
            </column>
        </createTable>

        <addForeignKeyConstraint baseTableName="authorities"
                                 baseColumnNames="user_id"
                                 constraintName="fk_authorities_user"
                                 referencedTableName="users"
                                 referencedColumnNames="id"/>
        <addForeignKeyConstraint baseTableName="products"
                                 baseColumnNames="supplier_id"
                                 constraintName="fk_product_supplier"
                                 referencedTableName="suppliers"
                                 referencedColumnNames="id"/>
        <addForeignKeyConstraint baseTableName="token_blacklist"
                                 baseColumnNames="user_id"
                                 constraintName="fk_token_blacklist_user"
                                 referencedTableName="users"
                                 referencedColumnNames="id"/>
        <addForeignKeyConstraint baseTableName="extra_order_stores"
                                 baseColumnNames="extra_order_id"
                                 constraintName="fk_extra_order_stores_extra_order"
                                 referencedTableName="extra_orders"
                                 referencedColumnNames="id"/>
        <addForeignKeyConstraint baseTableName="extra_order_products"
                                 baseColumnNames="product_id"
                                 constraintName="fk_extra_order_product_product"
                                 referencedTableName="products"
                                 referencedColumnNames="id"/>
        <addForeignKeyConstraint baseTableName="extra_order_products"
                                 baseColumnNames="extra_order_id"
                                 constraintName="fk_extra_order_product_extra_order"
                                 referencedTableName="extra_orders"
                                 referencedColumnNames="id"/>
        <addForeignKeyConstraint baseTableName="extra_orders"
                                 baseColumnNames="user_id"
                                 constraintName="fk_extra_order_user"
                                 referencedTableName="users"
                                 referencedColumnNames="id"/>
        <addForeignKeyConstraint baseTableName="extra_orders"
                                 baseColumnNames="supplier_id"
                                 constraintName="fk_extra_order_supplier"
                                 referencedTableName="suppliers"
                                 referencedColumnNames="id"/>
        <addForeignKeyConstraint baseTableName="extra_orders"
                                 baseColumnNames="stores"
                                 constraintName="fk_extra_order_stores"
                                 referencedTableName="extra_order_stores"
                                 referencedColumnNames="id"/>
        <addForeignKeyConstraint baseTableName="extra_orders"
                                 baseColumnNames="products"
                                 constraintName="fk_extra_order_products"
                                 referencedTableName="extra_order_products"
                                 referencedColumnNames="id"/>
    </changeSet>

    <changeSet id="2" author="PedroCostaa">
        <dropForeignKeyConstraint baseTableName="extra_orders"
                                  constraintName="fk_extra_order_stores"/>
        <dropColumn tableName="extra_orders" columnName="stores"/>

        <dropForeignKeyConstraint baseTableName="extra_orders"
                                  constraintName="fk_extra_order_products"/>
        <dropColumn tableName="extra_orders" columnName="products"/>
    </changeSet>

    <changeSet id="3" author="PedroCostaa">
        <createTable tableName="resources">
            <column name="id" type="BIGINT" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="user_id" type="BIGINT">
                <constraints nullable="false"/>
            </column>
            <column name="store" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="created_at" type="DATETIME">
                <constraints nullable="false"/>
            </column>
            <column name="processed" type="BOOLEAN">
                <constraints nullable="false"/>
            </column>
        </createTable>

        <addForeignKeyConstraint baseTableName="resources"
                                 baseColumnNames="user_id"
                                 constraintName="fk_resources_user"
                                 referencedTableName="users"
                                 referencedColumnNames="id"/>

        <createTable tableName="resource_products">
            <column name="id" type="BIGINT" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="product_id" type="BIGINT">
                <constraints nullable="false"/>
            </column>
            <column name="resource_id" type="BIGINT">
                <constraints nullable="false"/>
            </column>
            <column name="quantity" type="INT">
                <constraints nullable="false"/>
            </column>
        </createTable>

        <addForeignKeyConstraint baseTableName="resource_products"
                                 baseColumnNames="product_id"
                                 constraintName="fk_resource_product_product"
                                 referencedTableName="products"
                                 referencedColumnNames="id"/>

        <addForeignKeyConstraint baseTableName="resource_products"
                                 baseColumnNames="resource_id"
                                 constraintName="fk_resource_product_resource"
                                 referencedTableName="resources"
                                 referencedColumnNames="id"/>
    </changeSet>

    <changeSet id="4" author="PedroCostaa">
        <addColumn tableName="products">
            <column name="is_resource" type="BOOLEAN" defaultValue="0">
                <constraints nullable="false"/>
            </column>
        </addColumn>
    </changeSet>

    <changeSet id="5" author="PedroCostaa">
        <dropColumn tableName="users" columnName="first_access"/>
    </changeSet>

    <changeSet id="6" author="PedroCostaa">
        <dropForeignKeyConstraint baseTableName="resource_products" constraintName="fk_resource_product_resource"/>
        <addForeignKeyConstraint baseTableName="resource_products"
                                 baseColumnNames="resource_id"
                                 constraintName="fk_resource_product_resource"
                                 referencedTableName="resources"
                                 referencedColumnNames="id"
                                 deleteCascade="true"/>
    </changeSet>

    <changeSet id="7" author="PedroCostaa">
        <addColumn tableName="resources">
            <column name="qtt_received" type="INT"/>
        </addColumn>
        <addColumn tableName="resources">
            <column name="order_number" type="BIGINT"/>
        </addColumn>
    </changeSet>

    <changeSet id="8" author="PedroCostaa">
        <dropColumn tableName="resources" columnName="qtt_received"/>

        <addColumn tableName="resource_products">
            <column name="qtt_received" type="INT"/>
        </addColumn>
    </changeSet>
    
    <changeSet id="9" author="PedroCostaa">
        <addColumn tableName="resource_products">
            <column name="qttSent" type="INT"/>
        </addColumn>
    </changeSet>

    <changeSet id="10" author="PedroCostaa">
        <renameColumn tableName="resource_products" oldColumnName="qttSent" newColumnName="qtt_sent" columnDataType="INT"/>
    </changeSet>

    <changeSet id="11" author="PedroCostaa">
        <createTable tableName="supplier_registration">
            <column name="id" type="BIGINT" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="user_id" type="BIGINT">
                <constraints nullable="false"/>
            </column>
            <column name="created_at" type="DATETIME">
                <constraints nullable="false"/>
            </column>
            <column name="accepted" type="BOOLEAN">
                <constraints nullable="false"/>
            </column>
            <column name="type" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="cnpj" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="company_name" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="payment_term" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="seller_name" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="seller_phone" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="seller_email" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="address" type="VARCHAR(255)"/>
            <column name="factory_website" type="VARCHAR(255)"/>
            <column name="exchange" type="BOOLEAN"/>
            <column name="exchange_physical" type="BOOLEAN"/>
            <column name="price_table_file_path" type="VARCHAR(255)"/>
            <column name="price_table_file_name" type="VARCHAR(255)"/>
            <column name="catalog_file_path" type="VARCHAR(255)"/>
            <column name="catalog_file_name" type="VARCHAR(255)"/>
            <column name="sample_date" type="DATE"/>
            <column name="obs" type="VARCHAR(255)"/>
            <column name="supplier_website" type="VARCHAR(255)"/>
            <column name="minimum_order_value" type="DOUBLE"/>
            <column name="weekly_quotation" type="VARCHAR(255)"/>
            <column name="investments_on_store" type="BOOLEAN"/>
            <column name="purchase_gondola" type="BOOLEAN"/>
            <column name="participate_in_insert" type="BOOLEAN"/>
            <column name="birthday_party" type="BOOLEAN"/>
            <column name="other_participation" type="BOOLEAN"/>
            <column name="negotiate_bonus_on_first_purchase" type="BOOLEAN"/>
        </createTable>

        <addForeignKeyConstraint baseTableName="supplier_registration"
                                 baseColumnNames="user_id"
                                 constraintName="fk_supplier_registration_user"
                                 referencedTableName="users"
                                 referencedColumnNames="id"/>
    </changeSet>

    <changeSet id="12" author="PedroCostaa">
        <createTable tableName="supplier_registration_stores">
            <column name="id" type="BIGINT" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="supplier_registration_id" type="BIGINT">
                <constraints nullable="false"/>
            </column>
            <column name="store" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="delivery_time" type="INT">
                <constraints nullable="false"/>
            </column>
            <column name="seller_name" type="VARCHAR(255)"/>
            <column name="seller_phone" type="VARCHAR(255)"/>
            <column name="order_best_day" type="VARCHAR(255)"/>
            <column name="routine" type="VARCHAR(255)"/>
            <column name="motive" type="VARCHAR(255)"/>
        </createTable>

        <addForeignKeyConstraint baseTableName="supplier_registration_stores"
                                 baseColumnNames="supplier_registration_id"
                                 constraintName="fk_supplier_registration_stores_supplier_registration"
                                 referencedTableName="supplier_registration"
                                 referencedColumnNames="id"/>
    </changeSet>

    <changeSet id="13" author="PedroCostaa">
        <dropColumn tableName="supplier_registration" columnName="price_table_file_name"/>
        <dropColumn tableName="supplier_registration" columnName="catalog_file_name"/>
    </changeSet>

    <changeSet id="14" author="PedroCostaa">
        <dropNotNullConstraint tableName="supplier_registration" columnName="seller_name" columnDataType="VARCHAR(255)"/>
    </changeSet>

</databaseChangeLog>